    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Launch Proto Example",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "sourceMaps": true,
            "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
            "args": ["${workspaceFolder}/bin/zeus.ts", "gen","--zeus", "/Users/sxia/code/secret/zeus/main.zeus"]
        },
        {
            "name": "Debug Jest(current)",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/jest",
            "args": [
                "${fileBasenameNoExtension}",
                "--runInBand",
                "--coverage=false",
                "--no-cache",
            ],
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "sourceMaps": true,
        }
    ]
}